#ifndef SIMPLEKERNEL_SRC_LIBCXX_INCLUDE_SK_NEW_
#define SIMPLEKERNEL_SRC_LIBCXX_INCLUDE_SK_NEW_

#include <cstddef>

[[nodiscard]] void* operator new(size_t);

[[nodiscard]] void* operator new[](size_t);

[[nodiscard]] void* operator new(size_t, size_t) noexcept;

[[nodiscard]] void* operator new[](size_t, size_t) noexcept;

void operator delete(void*) noexcept;

void operator delete(void*, size_t) noexcept;

void operator delete[](void*) noexcept;

void operator delete[](void*, size_t) noexcept;

void operator delete(void*, size_t, size_t) noexcept;

void operator delete[](void*, size_t, size_t) noexcept;

#endif /* SIMPLEKERNEL_SRC_LIBCXX_INCLUDE_SK_NEW_ */
